<%= form_for [@omise, @review]  do |f| %>
<div class="form-group">
  <%= f.label :rating , "評価" %><br>
  <%= f.text_field :rating , class: "form-control" %>
</div>
<div class="form-group">
<%= f.label :body , "口コミ" %><br>
<%= f.text_area :body, class: "form-control" %>
</div>
<%= f.submit %>
<% end -%>
