<p id="notice"><%= notice %></p>

<h1>Listing Omises</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Tel</th>
      <th>Address</th>
      <th>Homepage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @omises.each do |omise| %>
      <tr>
        <td><%= omise.name %></td>
        <td><%= omise.tel %></td>
        <td><%= omise.address %></td>
        <td><%= omise.homepage %></td>
        <td><%= link_to 'Show', omise %></td>
        <td><%= link_to 'Edit', edit_omise_path(omise) %></td>
        <td><%= link_to 'Destroy', omise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Omise', new_omise_path %>
